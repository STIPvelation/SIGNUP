<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_header_section"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_back_black_24dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:paddingTop="12dp"
                android:paddingStart="@dimen/common_side_margin_20dp"
                android:paddingEnd="@dimen/common_side_margin_20dp"
                android:paddingBottom="12dp"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_sign_up_kyc_inform_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/text_black_14181B_100"
            android:fontFamily="@font/pretendard_medium_font_family"
            app:layout_constraintTop_toBottomOf="@+id/cl_header_section"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="@dimen/common_side_margin_20dp"
            android:layout_marginStart="@dimen/common_side_margin_20dp"
            tools:text="@string/sign_up_kyc_inform_title"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_sign_up_kyc_inform_passport_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/sign_up_kyc_inform_passport_name"
            android:textSize="14sp"
            android:textColor="@color/text_black_14181B_100"
            android:fontFamily="@font/pretendard_regular_font_family"
            app:layout_constraintTop_toBottomOf="@+id/tv_sign_up_kyc_inform_title"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="@dimen/common_side_margin_20dp"
            android:layout_marginStart="@dimen/common_side_margin_20dp"
            />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/et_nick_sign_up_kyc_inform_passport_last_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/text_black_14181B_100"
            android:fontFamily="@font/pretendard_regular_font_family"
            android:hint="@string/sign_up_kyc_inform_passport_last_name_hint"
            android:textColorHint="@color/text_gray_B0B8C1_100"
            android:background="@drawable/round_solid_gray_f4f7fc_radius_4dp"
            android:maxLines="1"
            android:inputType="textVisiblePassword"
            app:layout_constraintTop_toBottomOf="@+id/tv_sign_up_kyc_inform_passport_name"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="8dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:padding="12dp"
            />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/et_nick_sign_up_kyc_inform_passport_first_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/text_black_14181B_100"
            android:fontFamily="@font/pretendard_regular_font_family"
            android:hint="@string/sign_up_kyc_inform_passport_first_name_hint"
            android:textColorHint="@color/text_gray_B0B8C1_100"
            android:background="@drawable/round_solid_gray_f4f7fc_radius_4dp"
            android:maxLines="1"
            android:inputType="textVisiblePassword"
            app:layout_constraintTop_toBottomOf="@+id/et_nick_sign_up_kyc_inform_passport_last_name"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="8dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:padding="12dp"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_sign_up_kyc_inform_current_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/sign_up_kyc_inform_current_address"
            android:textSize="14sp"
            android:textColor="@color/text_black_14181B_100"
            android:fontFamily="@font/pretendard_regular_font_family"
            app:layout_constraintTop_toBottomOf="@+id/et_nick_sign_up_kyc_inform_passport_first_name"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="46dp"
            android:layout_marginStart="@dimen/common_side_margin_20dp"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_sign_up_kyc_inform_current_address_value"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/text_black_14181B_100"
            android:fontFamily="@font/pretendard_regular_font_family"
            android:hint="@string/sign_up_kyc_inform_current_address_hint"
            android:textColorHint="@color/text_gray_B0B8C1_100"
            android:background="@drawable/round_solid_gray_f4f7fc_radius_4dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_sign_up_kyc_inform_current_address"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="8dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:padding="12dp"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/btn_confirm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/common_confirm"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:textAlignment="center"
            android:background="@drawable/selector_gray_b0b8c1_sky_30c6e8_round_4dp"
            android:enabled="false"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:paddingTop="16dp"
            android:paddingBottom="16dp"
            android:layout_marginTop="@dimen/common_side_margin_20dp"
            android:layout_marginStart="@dimen/common_side_margin_20dp"
            android:layout_marginEnd="@dimen/common_side_margin_20dp"
            android:layout_marginBottom="@dimen/common_side_margin_20dp"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>