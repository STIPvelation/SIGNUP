<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        >

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_sign_up_kyc_address_zip_code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/text_red_DD3D43_100"
            android:fontFamily="@font/pretendard_regular_font_family"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp"
            android:layout_marginStart="@dimen/common_side_margin_20dp"
            tools:text="06132"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_sign_up_kyc_new_address_section"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/round_solid_gray_f4f7fc_radius_4dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_sign_up_kyc_address_zip_code"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/cl_sign_up_kyc_old_address_section"
            android:layout_marginTop="12dp"
            android:layout_marginStart="@dimen/common_side_margin_20dp"
            android:layout_marginEnd="@dimen/common_side_margin_20dp"
            app:layout_goneMarginBottom="@dimen/common_side_margin_20dp"
            app:layout_goneMarginTop="@dimen/common_side_margin_20dp"
            >

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_sign_up_kyc_address_new"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/sign_up_kyc_address_new"
                android:textSize="14sp"
                android:textColor="@color/text_sky_30C6E8_100"
                android:fontFamily="@font/pretendard_regular_font_family"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginTop="12dp"
                android:layout_marginStart="12dp"
                android:layout_marginBottom="12dp"
                />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_sign_up_kyc_address_new_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textColor="@color/text_black_14181B_100"
                android:fontFamily="@font/pretendard_medium_font_family"
                app:layout_constraintTop_toTopOf="@+id/tv_sign_up_kyc_address_new"
                app:layout_constraintStart_toEndOf="@+id/tv_sign_up_kyc_address_new"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="@+id/tv_sign_up_kyc_address_new"
                android:layout_marginTop="12dp"
                android:layout_marginStart="@dimen/common_side_margin_20dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="12dp"
                tools:text="서울 강남구 테헤란로 151(역삼하이츠빌딩) "
                />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_sign_up_kyc_old_address_section"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/round_solid_gray_f4f7fc_radius_4dp"
            app:layout_constraintTop_toBottomOf="@+id/cl_sign_up_kyc_new_address_section"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="6dp"
            android:layout_marginStart="@dimen/common_side_margin_20dp"
            android:layout_marginEnd="@dimen/common_side_margin_20dp"
            android:layout_marginBottom="@dimen/common_side_margin_20dp"
            app:layout_goneMarginTop="@dimen/common_side_margin_20dp"
            >

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_sign_up_kyc_address_old"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/sign_up_kyc_address_old"
                android:textSize="14sp"
                android:textColor="@color/text_sky_30C6E8_100"
                android:fontFamily="@font/pretendard_regular_font_family"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginTop="12dp"
                android:layout_marginStart="12dp"
                android:layout_marginBottom="12dp"
                />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_sign_up_kyc_address_old_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textColor="@color/text_black_14181B_100"
                android:fontFamily="@font/pretendard_medium_font_family"
                app:layout_constraintTop_toTopOf="@+id/tv_sign_up_kyc_address_old"
                app:layout_constraintStart_toEndOf="@+id/tv_sign_up_kyc_address_old"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="@+id/tv_sign_up_kyc_address_old"
                android:layout_marginTop="12dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="12dp"
                tools:text="서울 강남구 테헤란로 151(역삼하이츠빌딩) "
                />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>