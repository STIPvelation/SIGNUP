<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        >

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_sign_up_kyc_address_zip_code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/text_sky_30C6E8_100"
            android:fontFamily="@font/pretendard_regular_font_family"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp"
            android:layout_marginStart="@dimen/common_side_margin_20dp"
            tools:text="01632"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_sign_up_kyc_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/text_black_14181B_100"
            android:fontFamily="@font/pretendard_regular_font_family"
            app:layout_constraintTop_toBottomOf="@+id/tv_sign_up_kyc_address_zip_code"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp"
            android:layout_marginStart="@dimen/common_side_margin_20dp"
            tools:text="서울 강남구 테헤란로 151 (역삼하이츠빌딩)"
            />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/et_sign_up_kyc_search_address_detail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/text_black_14181B_100"
            android:fontFamily="@font/pretendard_regular_font_family"
            android:hint="@string/sign_up_kyc_search_address_detail_hint"
            android:textColorHint="@color/text_gray_B0B8C1_100"
            android:background="@drawable/round_stroke_gray_949698_solid_white_radius_4dp"
            android:maxLines="1"
            android:inputType="text"
            app:layout_constraintTop_toBottomOf="@+id/tv_sign_up_kyc_address"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="32dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:paddingTop="16dp"
            android:paddingStart="11dp"
            android:paddingEnd="11dp"
            android:paddingBottom="16dp"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/btn_confirm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/common_confirm"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:textAlignment="center"
            android:background="@drawable/selector_gray_b0b8c1_sky_30c6e8_round_4dp"
            android:enabled="false"
            app:layout_constraintTop_toBottomOf="@+id/et_sign_up_kyc_search_address_detail"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:paddingTop="16dp"
            android:paddingBottom="16dp"
            android:layout_marginTop="12dp"
            android:layout_marginStart="@dimen/common_side_margin_20dp"
            android:layout_marginEnd="@dimen/common_side_margin_20dp"
            android:layout_marginBottom="12dp"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>