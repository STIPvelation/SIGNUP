<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/item_1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="16dp"
    android:background="?android:attr/selectableItemBackground"
    android:padding="12dp">

    <TextView
        android:id="@+id/item_1_text_asset_name"
        style="@style/DefaultTextStyle_16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/label_etip_ai"
        app:layout_constraintBottom_toTopOf="@+id/item_1_separator"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/item_1_text_pl_value"
        style="@style/IP_Holding_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/mark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/item_1_text_pl_label"
        style="@style/IP_Holding_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/profit_loss"
        app:layout_constraintBaseline_toBaselineOf="@id/item_1_text_pl_value"
        app:layout_constraintEnd_toStartOf="@id/item_1_text_pl_value" />

    <TextView
        android:id="@+id/item_1_text_return_rate_value"
        style="@style/IP_Holding_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/mark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_1_text_pl_value" />

    <TextView
        android:id="@+id/item_1_text_return_rate_label"
        style="@style/IP_Holding_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/w_ratio"
        app:layout_constraintBaseline_toBaselineOf="@id/item_1_text_return_rate_value"
        app:layout_constraintEnd_toStartOf="@id/item_1_text_return_rate_value" />

    <View
        android:id="@+id/item_1_separator"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="12dp"
        android:background="#E0E0E0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_1_text_return_rate_label" />

    <TextView
        android:id="@+id/item_1_text_quantity_value"
        style="@style/IP_Holding_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/mark"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_1_separator" />

    <TextView
        android:id="@+id/item_1_text_quantity_label"
        style="@style/IP_Holding_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/quantity"
        app:layout_constraintStart_toStartOf="@id/item_1_text_quantity_value"
        app:layout_constraintTop_toBottomOf="@id/item_1_text_quantity_value" />

    <TextView
        android:id="@+id/item_1_text_avg_price_value"
        style="@style/IP_Holding_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/mark"
        app:layout_constraintBaseline_toBaselineOf="@id/item_1_text_quantity_value"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/item_1_text_avg_price_label"
        style="@style/IP_Holding_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/average_buy"
        app:layout_constraintEnd_toEndOf="@id/item_1_text_avg_price_value"
        app:layout_constraintTop_toBottomOf="@id/item_1_text_avg_price_value" />

    <TextView
        android:id="@+id/item_1_text_valuation_value"
        style="@style/IP_Holding_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/mark"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_1_text_quantity_label" />

    <TextView
        android:id="@+id/item_1_text_valuation_label"
        style="@style/IP_Holding_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/evaluated_price"
        app:layout_constraintStart_toStartOf="@id/item_1_text_valuation_value"
        app:layout_constraintTop_toBottomOf="@id/item_1_text_valuation_value" />

    <TextView
        android:id="@+id/item_1_text_purchase_amount_value"
        style="@style/IP_Holding_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/mark"
        app:layout_constraintBaseline_toBaselineOf="@id/item_1_text_valuation_value"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/item_1_text_purchase_amount_label"
        style="@style/IP_Holding_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/buy_price"
        app:layout_constraintEnd_toEndOf="@id/item_1_text_purchase_amount_value"
        app:layout_constraintTop_toBottomOf="@id/item_1_text_purchase_amount_value" />

</androidx.constraintlayout.widget.ConstraintLayout>